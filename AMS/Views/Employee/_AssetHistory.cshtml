@model AMS.Models.EmployeeViewModel.EmployeeCRUDViewModel

<table class="CustomBlueTable">
    <thead>
        <tr>
            <th>Id</th>
            <th>Asset Id</th>
            <th>Assign Employee</th>
            <th>Action</th>
            <th>Note</th>
            <th>Created Date</th>
        </tr>
    </thead>
    <tbody>
        @{ foreach (var item in Model.listAssetHistoryCRUDViewModel)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.AssetId</td>
                    <td>@item.AssignEmployeeDisplay</td>
                    <td>@item.Action</td>
                    <td>@item.Note</td>
                    <td>@String.Format("{0:f}", item.CreatedDate)</td>
                </tr>
            } }
    </tbody>
</table>


